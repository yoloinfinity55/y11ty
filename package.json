{
  "name": "y11ty",
  "version": "1.0.0",
  "description": "A modern blog built with Eleventy and Tailwind CSS",
  "type": "module",
  "scripts": {
    "dev": "eleventy --serve --watch",
    "build": "eleventy",
    "serve": "eleventy --serve",
    "start": "npm run dev",
    "clean": "rm -rf _site",
    "build:production": "npm run clean && eleventy",
    "debug": "DEBUG=Eleventy* eleventy --serve"
  },
  "keywords": [
    "eleventy",
    "11ty",
    "blog",
    "static-site",
    "tailwindcss",
    "jamstack"
  ],
  "author": "Y11ty Blog",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/yoloinfinity55/y11ty.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "eleventy-plugin-tailwindcss-4": "^2.0.1",
    "tailwindcss": "^4.1.14"
  }
}